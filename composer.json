{
    "name": "iodigital-com/php-code-sniffer-baseliner",
    "description": "IO PHP Code Sniffer Baseliner",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "~8.1.0 || ~8.2.0 || ~8.3.0",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "IODigital\\CodeSnifferBaseliner\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IODigital\\CodeSnifferBaseliner\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/phpcs-baseliner"
    ],
    "require-dev": {
        "iodigital-com/php-code-sniffer-standard": "^29.2",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.6.8",
        "phpstan/phpstan-phpunit": "^1.1.1",
        "phpstan/phpstan-strict-rules": "^1.2.3",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        }
    }
}
